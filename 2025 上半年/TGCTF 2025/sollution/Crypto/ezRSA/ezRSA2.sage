def part_roots(n, c, e, p):
    # å°†eçº¦ç®€åˆ°å¯é€†ï¼Œgä¸ºä½™ä¸‹çš„å› æ•°
    def div_e(e, p):
        g = GCD(e, (p - 1))
        while GCD(e, (p - 1)) != 1:
            e //= GCD(e, (p - 1))
            g *= GCD(e, (p - 1))
        return e, g
    
    # çº¦ç®€eï¼Œä¾¿äºå¼€æ ¹
    e, g = div_e(e, p)
    d = inverse(e, p - 1)
    M = pow(c, d, p)
    
    # åœ¨æœ‰é™åŸŸå†…å¼€æ ¹
    R.<x> = Zmod(p)[]
    f = x ^ g - M
    return [int(i[0]) for i in f.monic().roots()]

